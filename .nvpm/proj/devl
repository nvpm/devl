# proj {

workspace proj
  tab proj
    buff auto: plug/proj/autoload/proj.vim
    buff plug: plug/proj/plugin/proj.vim
    buff pf: .nvpm/proj/proj
  tab test
    buff init: test/init.vim
    buff nvpm: /i/proj/save/nvpm/plugin/nvpm.vim
   *buff patt: /i/apps/share/nvim/runtime/doc/pattern.txt
  *tab meta
    buff vers: plug/proj/version
    buff docx: plug/proj/doc/proj.txt
    buff read: plug/proj/README.md

# }
# flux {

workspace flux
  tab flux
    buff auto: plug/flux/autoload/flux.vim
    buff pf: .nvpm/proj/proj
   *buff data: data/flux/data
   *buff seng: seng/proj/data.sng
   *buff plug: plug/flux/plugin/flux.vim
  tab test
    buff init: test/init.vim
    buff pf: .nvpm/proj/proj
   *buff nvpm: /i/proj/save/nvpm/plugin/nvpm.vim
   *buff patt: /i/apps/share/nvim/runtime/doc/pattern.txt
  tab meta
    buff vers: plug/flux/version
   *buff docx: plug/flux/doc/flux.txt
   *buff read: plug/flux/README.md

# }
# zoom {

*workspace zoom
  tab zoom
    buff auto: plug/zoom/autoload/zoom.vim
    buff plug: plug/zoom/plugin/zoom.vim
  tab meta
    buff docx: plug/zoom/doc/zoom.txt
    buff read: plug/zoom/README.md
    buff vers: plug/zoom/version
    buff make: makefile

# }
# text {

*workspace text
  tab text
    buff auto: plug/text/autoload/text.vim
    buff plug: plug/text/plugin/text.vim
  tab meta
   *buff docx: plug/text/doc/text.txt
   *buff read: plug/text/README.md
    buff vers: plug/text/version

# }
# meta {

workspace meta
  tab conf
    buff devl: conf/devl.vim
    buff user: conf/user.vim
  tab main
    buff make: makefile
    buff repo: repo
  tab docs
    buff read: README.md
# }
