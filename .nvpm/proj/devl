  tab test
    buff auto: plug/test/autoload/test.vim
   *buff plug: plug/test/plugin/test.vim
  tab meta
    buff vers: plug/test/version
   *buff docx: plug/test/doc/test.txt
   *buff read: plug/test/README.md

# }
# text {

*workspace text
  tab text
    buff auto: plug/text/autoload/text.vim
    buff plug: plug/text/plugin/text.vim
  tab meta
   *buff docx: plug/text/doc/text.txt
   *buff read: plug/text/README.md
    buff vers: plug/text/version

# }
# file {

*workspace file
  tab file
    buff auto: plug/file/autoload/file.vim
   *buff plug: plug/file/plugin/file.vim
  tab meta
    buff vers: plug/file/version
   *buff docx: plug/file/doc/file.txt
   *buff read: plug/file/README.md

# }
# meta {

*workspace meta
  tab conf
    buff devl: conf/devl.vim
    buff user: conf/user.vim
  tab main
    buff make: makefile
    buff repo: repo
  tab docs
    buff read: README.md
# }
